%section.sign_in.container
  .row
    .col-sm-10.col-sm-offset-1
      = bootstrap_form_tag url:'/login' do |f|
        = f.email_field :email
        = f.password_field :password 
        = f.submit "Log In" 
        = link_to "Forgot your password?",forgot_password_path, class: "pull-right"
         
 